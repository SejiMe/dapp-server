{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.EntityFrameworkCore": "Information",
        "System": "Information",
        "dengue.watch.api": "Debug"
      }
    }
  },
  "Supabase": {
    "Url": "https://your-project.supabase.co",
    "AnonKey": "your-anon-key-here",
    "JwtSecret": "your-jwt-secret-here"
  },
  "Postgres": {
    "Host": "localhost",
    "Port": 5432,
    "Database": "dengue_watch_dev",
    "Username": "postgres",
    "Password": "password",
    "SslModeRequire": false,
    "TrustServerCertificate": true,
    "UserId": "",
    "Server": "",
    "ServerPort": ""
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "Development Quartz ASP.NET Core Sample Scheduler"
  },
  "Jobs": {
  "DailyWeatherPooling": {
    "Cron": "0 30 16 * * ?"
  }
}
}
